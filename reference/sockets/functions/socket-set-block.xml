  <refentry xml:id="function.socket-set-block" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>socket_set_block</refname>
    <refpurpose>
     Configura o modo bloqueio em um socket resource
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descrição</title>
     <methodsynopsis>
      <type>bool</type><methodname>socket_set_block</methodname>
      <methodparam><type>resource</type><parameter>socket</parameter></methodparam>
     </methodsynopsis>
    
    &warn.undocumented.func;
    
    <para>
     &return.success;
    </para>
    <para>
     Veja também <function>socket_set_nonblock</function> e 
     <function>socket_set_option</function>
    </para>
   </refsect1>
  </refentry>

