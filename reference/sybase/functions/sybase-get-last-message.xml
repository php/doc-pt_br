<refentry xml:id="function.sybase-get-last-message" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>sybase_get_last_message</refname>
  <refpurpose>Retorna a última mensagem do servidor</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>sybase_get_last_message</methodname>
   <void/>
  </methodsynopsis>
  <para> 
   <function>sybase_get_last_message</function> retorna a última
   mensagem reportada pelo servidor.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the message as a string.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>sybase_min_message_severity</function></member>
   </simplelist>
  </para>
 </refsect1>
 
</refentry>

