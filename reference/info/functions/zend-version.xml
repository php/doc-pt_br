  <refentry xml:id="function.zend-version" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>zend_version</refname>
    <refpurpose>Obtém a versão da Zend engine que esta sendo executada</refpurpose>
   </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
     <methodsynopsis>
      <type>string</type><methodname>zend_version</methodname>
      <void/>
     </methodsynopsis>
    <para>
     Retorna uma string contendo a versão atual da Zend Engine que esta sendo executada.
    </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna o número da versão da Zend Engine, como uma string.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
    <para>
     <example>
      <title>Exemplo <function>zend_version</function></title>
      <programlisting role="php">
<![CDATA[
<?php
echo "Zend engine version: " . zend_version();
?>
]]>
      </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
Zend engine version: 2.2.0
]]>
    </screen>
     </example>
    </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
    <para>
     <simplelist>
      <member><function>phpinfo</function></member>
      <member><function>phpcredits</function></member>
      <member><function>php_logo_guid</function></member>
      <member><function>phpversion</function></member>
     </simplelist>
    </para>
   </refsect1>
   
  </refentry>

