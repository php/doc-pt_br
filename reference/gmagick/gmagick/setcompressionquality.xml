<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="gmagick.setcompressionquality">
 <refnamediv>
  <refname>Gmagick::setCompressionQuality</refname>
  <refpurpose>Define a qualidade de compactação padrão do objeto</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="oop">
   <type>Gmagick</type><methodname>Gmagick::setCompressionQuality</methodname>
   <methodparam>
    <type>int</type><parameter>quality</parameter>
   </methodparam>
  </methodsynopsis>
  <simpara>
   Define a qualidade de compactação padrão do objeto.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>quality</parameter></term>
    <listitem>
     <simpara>
      O valor padrão de GraphicsMagick é 75.
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>


 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   O objeto <classname>Gmagick</classname>.
  </simpara>
 </refsect1>
 <refsect1 role="errors">
  &reftitle.errors;
  <simpara>
   &gmagick.gmagickexception.throw;
  </simpara>
 </refsect1>

