 <refnamediv>
  <refname>Gmagick::getimageformat</refname>
  <refpurpose>Retorna o formato de uma imagem específica em uma sequência</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>string</type><methodname>Gmagick::getimageformat</methodname>
   <void />
  </methodsynopsis>
  <para>
   Retorna o formato de uma imagem específica em uma sequência.
  </para>



 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna uma string contendo o formato da imagem em caso de sucesso.
  </para>
 </refsect1>

