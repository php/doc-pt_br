<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="gmagick.blurimage">
 <refnamediv>
  <refname>Gmagick::blurimage</refname>
  <refpurpose>Adiciona filtro de desfoque à imagem</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>Gmagick</type><methodname>Gmagick::blurimage</methodname>
   <methodparam><type>float</type><parameter>radius</parameter></methodparam>
   <methodparam><type>float</type><parameter>sigma</parameter></methodparam>
   <methodparam choice="opt"><type>int</type><parameter>channel</parameter></methodparam>
  </methodsynopsis>
  <para>
   Adiciona filtro de desfoque à imagem.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>radius</parameter></term>
    <listitem>
     <para>
      Raio de desfoque
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>sigma</parameter></term>
    <listitem>
     <para>
      Desvio padrão
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   O objeto <classname>Gmagick</classname> desfocado
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   &gmagick.gmagickexception.throw;
  </para>
 </refsect1>
</refentry>
