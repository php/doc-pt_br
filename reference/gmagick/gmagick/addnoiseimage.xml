<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="gmagick.addnoiseimage">
 <refnamediv>
  <refname>Gmagick::addnoiseimage</refname>
  <refpurpose>Adiciona ruído aleatório à imagem</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>Gmagick</type><methodname>Gmagick::addnoiseimage</methodname>
   <methodparam><type>int</type><parameter>noise_type</parameter></methodparam>
  </methodsynopsis>
  <simpara>
    Adiciona ruído aleatório à imagem.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>noise_type</parameter></term>
    <listitem>
     <simpara>
      O tipo de ruído. Consulte esta lista de <link linkend="gmagick.constants.noise">constantes de ruído</link>.
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   O objeto Gmagick com o ruído adicionado.
  </simpara>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <simpara>
   &gmagick.gmagickexception.throw;
  </simpara>
 </refsect1>
</refentry>
