<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="gmagickdraw.gettextdecoration">
 <refnamediv>
  <refname>GmagickDraw::gettextdecoration</refname>
  <refpurpose>Retorna a decoração do texto</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>GmagickDraw::gettextdecoration</methodname>
   <void/>
  </methodsynopsis>
  <simpara>
   Retorna a decoração aplicada ao anotar com texto.
  </simpara>



 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   Retorna uma das constantes DECORATION_ ou 0 se nenhuma decoração estiver definida.
  </simpara>
 </refsect1>

</refentry>
