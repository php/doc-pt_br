<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="gmagickdraw.ellipse">
 <refnamediv>
  <refname>GmagickDraw::ellipse</refname>
  <refpurpose>Desenha uma elipse na imagem</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>GmagickDraw</type><methodname>GmagickDraw::ellipse</methodname>
   <methodparam><type>float</type><parameter>ox</parameter></methodparam>
   <methodparam><type>float</type><parameter>oy</parameter></methodparam>
   <methodparam><type>float</type><parameter>rx</parameter></methodparam>
   <methodparam><type>float</type><parameter>ry</parameter></methodparam>
   <methodparam><type>float</type><parameter>start</parameter></methodparam>
   <methodparam><type>float</type><parameter>end</parameter></methodparam>
  </methodsynopsis>
  <simpara>
   Desenha uma elipse na imagem.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>ox</parameter></term>
    <listitem>
     <simpara>
      Coordenada x da origem.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>oy</parameter></term>
    <listitem>
     <simpara>
      Coordenada y da origem.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>rx</parameter></term>
    <listitem>
     <simpara>
      Raio no eixo x.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>ry</parameter></term>
    <listitem>
     <simpara>
      Raio no eixo y.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>start</parameter></term>
    <listitem>
     <simpara>
      Ângulo inicial em graus.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>end</parameter></term>
    <listitem>
     <simpara>
      Ângulo final em graus.
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   O objeto <classname>GmagickDraw</classname>.
  </simpara>
 </refsect1>


