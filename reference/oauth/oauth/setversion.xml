<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="oauth.setversion">
 <refnamediv>
  <refname>OAuth::setVersion</refname>
  <refpurpose>Define a versão OAuth</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>bool</type><methodname>OAuth::setVersion</methodname>
   <methodparam><type>string</type><parameter>version</parameter></methodparam>
  </methodsynopsis>
  <para>
   Define a versão OAuth para requisições subsequentes.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>version</parameter></term>
    <listitem>
     <para>
      Versão OAuth, o valor padrão é sempre "1.0".
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

</refentry>
