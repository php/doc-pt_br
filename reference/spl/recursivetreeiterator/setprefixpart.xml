<refentry xml:id="recursivetreeiterator.setprefixpart" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>RecursiveTreeIterator::setPrefixPart</refname>
  <refpurpose>Define uma parte do prefixo</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="RecursiveTreeIterator">
   <modifier>public</modifier> <type>void</type><methodname>RecursiveTreeIterator::setPrefixPart</methodname>
   <methodparam><type>int</type><parameter>part</parameter></methodparam>
   <methodparam><type>string</type><parameter>value</parameter></methodparam>
  </methodsynopsis>
  <para>
   Define uma parte do prefixo usado na árvore gráfica.
  </para>

  &warn.undocumented.func;

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>part</parameter></term>
     <listitem>
      <para>
       Uma das constantes <link linkend="recursivetreeiterator.constants">RecursiveTreeIterator::PREFIX_*</link>.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>value</parameter></term>
     <listitem>
      <para>
       O valor a ser atribuído à parte do prefixo especificada em <parameter>part</parameter>.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>


</refentry>
