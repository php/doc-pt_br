<refentry xml:id="splfixedarray.jsonserialize" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>SplFixedArray::jsonSerialize</refname>
  <refpurpose>Retorna uma representação que pode ser convertida em JSON</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="SplFixedArray">
   <modifier>public</modifier> <type>mixed</type><methodname>SplFixedArray::jsonSerialize</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Serializa o array para um valor que pode ser serializado nativamente pelo
   <function>json_encode</function>.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna os dados do array que podem ser serializados pelo <function>json_encode</function>,
   que é um valor de qualquer tipo que não seja um &resource;.
  </para>
 </refsect1>

</refentry>
