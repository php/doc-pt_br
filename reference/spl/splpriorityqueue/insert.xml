<refentry xml:id="splpriorityqueue.insert" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SplPriorityQueue::insert</refname>
  <refpurpose>Insere um elemento na fila deslocando-o para cima</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="SplPriorityQueue">
   <modifier>public</modifier> <type>true</type><methodname>SplPriorityQueue::insert</methodname>
   <methodparam><type>mixed</type><parameter>value</parameter></methodparam>
   <methodparam><type>mixed</type><parameter>priority</parameter></methodparam>
  </methodsynopsis>

  <para>
   Insere o <parameter>value</parameter> com a prioridade <parameter>priority</parameter> na fila.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>value</parameter></term>
     <listitem>
      <para>
       O valor a ser inserido.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>priority</parameter></term>
     <listitem>
      <para>
       A prioridade associada.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.true.always;
  </para>
 </refsect1>

</refentry>
