<refentry xml:id="spldoublylinkedlist.unshift" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SplDoublyLinkedList::unshift</refname>
  <refpurpose>Acrescenta um elemento ao início da lista duplamente encadeada</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="SplDoublyLinkedList">
   <modifier>public</modifier> <type>void</type><methodname>SplDoublyLinkedList::unshift</methodname>
   <methodparam><type>mixed</type><parameter>value</parameter></methodparam>
  </methodsynopsis>

  <para>
   Acrescenta <parameter>value</parameter> ao início da lista duplamente encadeada.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>value</parameter></term>
     <listitem>
      <para>
       O valor a ser acrescentado.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>

</refentry>
