<refentry xml:id="phptoken.tostring" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>PhpToken::__toString</refname>
  <refpurpose>Retorna o conteúdo textual do token</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="PhpToken">
   <modifier>public</modifier> <type>string</type><methodname>PhpToken::__toString</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Retorna o conteúdo textual do token.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Um conteúdo textual do token.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <example>
   <title>Exemplo de <function>PhpToken::__toString</function></title>
   <programlisting role="php">
<![CDATA[
<?php
$token = new PhpToken(T_ECHO, 'echo');
echo $token;
]]>
   </programlisting>
   &examples.outputs;
   <screen>
<![CDATA[
echo
]]>
   </screen>
  </example>
 </refsect1>

 <refsect1 role="seealso">
