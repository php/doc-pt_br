<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="function.rrd-version">
 <refnamediv>
  <refname>rrd_version</refname>
  <refpurpose>Obtém informações sobre a biblioteca rrdtool subjacente</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>rrd_version</methodname>
   <void/>
  </methodsynopsis>
  <simpara>
   Retorna informações sobre a biblioteca rrdtool subjacente.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   String com o número da versão do rrdtool, por exemplo "1.4.3".
  </simpara>
 </refsect1>

</refentry>
