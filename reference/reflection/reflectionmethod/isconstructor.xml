<refentry xml:id="reflectionmethod.isconstructor" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ReflectionMethod::isConstructor</refname>
  <refpurpose>Verifica se o método é um construtor</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="ReflectionMethod">
   <modifier>public</modifier> <type>bool</type><methodname>ReflectionMethod::isConstructor</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Verifica se o método é um construtor.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &true; se o método for um construtor, caso contrário &false;
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>ReflectionMethod::__construct</methodname></member>
    <member><methodname>ReflectionMethod::isAbstract</methodname></member>
    <member><methodname>ReflectionMethod::isDestructor</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>
