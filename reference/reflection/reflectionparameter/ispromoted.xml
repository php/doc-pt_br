<refentry xml:id="reflectionparameter.ispromoted" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ReflectionParameter::isPromoted</refname>
  <refpurpose>Verifica se um parâmetro está promovido a uma propriedade</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="ReflectionParameter">
   <modifier>public</modifier> <type>bool</type><methodname>ReflectionParameter::isPromoted</methodname>
   <void/>
  </methodsynopsis>
  <simpara>
   Verifica se o parâmetro está
   <link linkend="language.oop5.decon.constructor.promotion">promovido</link>
   a uma propriedade.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;</refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   &true; se o parâmetro está promovido a uma propriedade, &false; caso contrário.
  </simpara>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <simplelist>
   <member><methodname>ReflectionProperty::isPromoted</methodname></member>
  </simplelist>
 </refsect1>

</refentry>
