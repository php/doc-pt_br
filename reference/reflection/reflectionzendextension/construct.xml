 <refnamediv>
  <refname>ReflectionZendExtension::__construct</refname>
  <refpurpose>Constrói um objeto ReflectionZendExtension</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <constructorsynopsis role="ReflectionZendExtension">
   <modifier>public</modifier> <methodname>ReflectionZendExtension::__construct</methodname>
   <methodparam><type>string</type><parameter>name</parameter></methodparam>
  </constructorsynopsis>
  <para>
   Constrói um novo objeto <type>ReflectionZendExtension</type>.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>name</parameter></term>
    <listitem>
     <para>
      O nome da extensão.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   Lança <classname>ReflectionException</classname> se a extensão a ser refletida não existir.
  </para>
 </refsect1>

</refentry>
