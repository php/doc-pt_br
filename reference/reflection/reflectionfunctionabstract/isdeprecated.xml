<refentry xml:id="reflectionfunctionabstract.isdeprecated" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ReflectionFunctionAbstract::isDeprecated</refname>
  <refpurpose>Verifica se está descontinuada</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="ReflectionFunctionAbstract">
   <modifier>public</modifier> <type>bool</type><methodname>ReflectionFunctionAbstract::isDeprecated</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Verifica se a função está descontinuada.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &true; se estiver descontinuada, caso contrário, &false;
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>
     Exemplo de <methodname>ReflectionFunctionAbstract::isDeprecated</methodname>
    </title>
    <programlisting role="php">
<![CDATA[
<?php
$rf = new ReflectionFunction('ereg');
var_dump($rf->isDeprecated());
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
bool(true)
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><classname>Deprecated</classname></member>
    <member><methodname>ReflectionFunctionAbstract::getDocComment</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>
