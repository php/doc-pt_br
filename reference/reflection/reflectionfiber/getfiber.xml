 <refnamediv>
  <refname>ReflectionFiber::getFiber</refname>
  <refpurpose>Obtém a instância do Fiber refletida</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="ReflectionFiber">
   <modifier>public</modifier> <type>Fiber</type><methodname>ReflectionFiber::getFiber</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Retorna a instância <classname>Fiber</classname> que está sendo refletida.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   A instância <classname>Fiber</classname> sendo refletida.
  </para>
 </refsect1>


