<refentry xml:id="reflectionextension.getversion" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ReflectionExtension::getVersion</refname>
  <refpurpose>Obtém a versão da extensão</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="ReflectionExtension">
   <modifier>public</modifier> <type class="union"><type>string</type><type>null</type></type><methodname>ReflectionExtension::getVersion</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Obtém a versão da extensão.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   A versão da extensão ou &null; se a extensão não tiver versão.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemplo de <methodname>ReflectionExtension::getVersion</methodname></title>
    <programlisting role="php">
<![CDATA[
<?php
$ext = new ReflectionExtension('mysqli');
var_dump($ext->getVersion());
?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
string(3) "0.1"
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>ReflectionExtension::info</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>
