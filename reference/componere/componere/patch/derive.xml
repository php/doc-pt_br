<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="componere-patch.derive">
 <refnamediv>
  <refname>Componere\Patch::derive</refname>
  <refpurpose>Derivaçaõ de Patch</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>Patch</type><methodname>Componere\Patch::derive</methodname>
   <methodparam><type>object</type><parameter>instance</parameter></methodparam>
  </methodsynopsis>
  <simpara>
   Deriva um <type>Patch</type> para a <parameter>instance</parameter> informada.
  </simpara>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>instance</parameter></term>
    <listitem>
     <simpara>
      O destino para o Patch derivado.
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   <type>Patch</type> para <parameter>instance</parameter> derivado do <type>Patch</type> atual.
  </simpara>
 </refsect1>

 <refsect1 role="exceptions">
  <title>Exceções</title>
  <warning>
   <simpara>
    Lança uma <type>InvalidArgumentException</type> se <parameter>instance</parameter> não for compatível.
   </simpara>
  </warning>
 </refsect1>

</refentry>
