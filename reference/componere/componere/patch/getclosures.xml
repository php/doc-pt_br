  <refpurpose>Obtém Closures</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>array</type><methodname>Componere\Patch::getClosures</methodname>
