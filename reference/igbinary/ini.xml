 &ini.descriptions.title;

 <variablelist>
 <varlistentry xml:id="ini.igbinary.compact-strings">
  <term>
   <parameter>igbinary.compact_strings</parameter>
   <type>bool</type>
  </term>
  <listitem>
   <simpara>
    Habilita ou desabilita a compactação de strings duplicadas.
    O padrão é On.
   </simpara>
  </listitem>
 </varlistentry>

 <varlistentry xml:id="ini.igbinary.save-handler">
  <term>
   <parameter>session.save_handler</parameter>
   <type>string</type>
  </term>
  <listitem>
   <simpara>
    Igbinary é usado como manipulador de sessão definindo este valor como <literal>igbinary</literal>.
   </simpara>
  </listitem>
 </varlistentry>

 </variablelist>
