<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="gearmanworker.setoptions">
 <refnamediv>
  <refname>GearmanWorker::setOptions</refname>
  <refpurpose>Define opções do trabalhador</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="GearmanWorker">
   <modifier>public</modifier> <type>true</type><methodname>GearmanWorker::setOptions</methodname>
   <methodparam><type>int</type><parameter>option</parameter></methodparam>
  </methodsynopsis>
  <simpara>
   Define uma ou mais opções para o valor fornecido.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>option</parameter></term>
    <listitem>
     <simpara>
      As opções a serem definidas
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   &return.true.always;
  </simpara>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <simplelist>
   <member><methodname>GearmanWorker::options</methodname></member>
   <member><methodname>GearmanWorker::addOptions</methodname></member>
   <member><methodname>GearmanWorker::removeOptions</methodname></member>
   <member><methodname>GearmanClient::setOptions</methodname></member>
  </simplelist>
 </refsect1>

</refentry>
