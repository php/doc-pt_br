<refentry xml:id="function.ui-draw-text-font-fontfamilies" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>UI\Draw\Text\Font\fontFamilies</refname>
  <refpurpose>Recupera Famílias de Fontes</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>array</type><methodname>UI\Draw\Text\Font\fontFamilies</methodname>
   <void />
  </methodsynopsis>
  <para>
   Retorna um array com famílias de fontes válidas para o sistema atual.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna um &array; de famílias de fontes válidas para o sistema atual.
  </para>
 </refsect1>

</refentry>

