<refentry xml:id="ui-controls-tab.append" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>UI\Controls\Tab::append</refname>
  <refpurpose>Anexa página</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>UI\Controls\Tab::append</methodname>
   <methodparam><type>string</type><parameter>name</parameter></methodparam>
   <methodparam><type>UI\Control</type><parameter>control</parameter></methodparam>
  </methodsynopsis>
  <para>
   Anexa uma nova página a este controle de guias.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>name</parameter></term>
    <listitem>
     <para>
      O nome para a nova página.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>control</parameter></term>
    <listitem>
     <para>
      O controle para a nova página.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna o índice do controle anexado, pode ser 0.
  </para>
 </refsect1>


</refentry>

