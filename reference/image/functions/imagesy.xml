  <refentry xml:id="function.imagesy" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>imagesy</refname>
    <refpurpose>Obtém a altura da imagem</refpurpose>
   </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
     <methodsynopsis>
      <type>int</type><methodname>imagesy</methodname>
      <methodparam><type>resource</type><parameter>image</parameter></methodparam>
     </methodsynopsis>
    <para>
     Retorna a altura do dado resource <parameter>image</parameter>.
    </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    &gd.image.description;
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna a altura da <parameter>image</parameter> ou &false; em 
   erro.
  </para>
 </refsect1>
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
    <example>
     <title>Usando <function>imagesy</function></title>
     <programlisting role="php">
<![CDATA[
<?php

// create a 300*200 image
$img = imagecreatetruecolor(300, 200);

echo imagesy($img); // 200

?>
]]>  
     </programlisting>
    </example>
  </para>
 </refsect1>
 <refsect1 role="seealso">
  &reftitle.seealso;
    <para>
     <simplelist>
      <member><function>imagecreatetruecolor</function></member>
      <member><function>getimagesize</function></member>
      <member><function>imagesx</function></member>
     </simplelist>
    </para>
   </refsect1>
  </refentry>

