<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="function.runkit7-superglobals">
 <refnamediv>
  <refname>runkit7_superglobals</refname>
  <refpurpose>
   Retorna um array indexado numericamente de superglobais registradas
  </refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>array</type><methodname>runkit7_superglobals</methodname>
   <void/>
  </methodsynopsis>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   Retorna um array indexada numericamente das superglobais atualmente registradas.
   ou seja, _GET, _POST, _REQUEST, _COOKIE, _SESSION, _SERVER, _ENV e _FILES.
  </simpara>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <simplelist>
   <member><link linkend="language.variables.scope">Escopo de vari√°veis</link></member>
  </simplelist>
 </refsect1>

</refentry>
