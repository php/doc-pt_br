<refentry xml:id="function.zip-close" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>zip_close</refname>
  <refpurpose>Fecha um arquivo ZIP</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>void</type><methodname>zip_close</methodname>
   <methodparam><type>resource</type><parameter>zip</parameter></methodparam>
  </methodsynopsis>
  <para>
   Fecha um dado arquivo ZIP.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>zip</parameter></term>
     <listitem>
      <para>
       Um arquivo ZIP previamente aberto com <function>zip_open</function>.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.void;
  </para>
 </refsect1>
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>zip_open</function></member>
    <member><function>zip_read</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

