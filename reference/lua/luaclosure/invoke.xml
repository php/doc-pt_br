 <refsect1 role="examples">
  &reftitle.examples;
  <example>
   <title>Exemplo de função <function>LuaClosure::__invoke</function></title>
   <programlisting role="php">
<![CDATA[
<?php
$lua = new Lua();
$closure = $lua->eval(<<<CODE
    return (function ()
        print("ola mundo")
    end)
CODE
);

$lua->call($closure);
$closure();
?>
]]>
   </programlisting>
   &example.outputs;
   <screen>
<![CDATA[
ola mundoola mundo
]]>
   </screen>
  </example>
