<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.readline-clear-history">
 <refnamediv>
  <refname>readline_clear_history</refname>
  <refpurpose>Limpa o histórico</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>true</type><methodname>readline_clear_history</methodname>
   <void/>
  </methodsynopsis>
  <simpara>
   Esta função limpa todo o histórico da linha de comando.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   &return.true.always;
  </simpara>
 </refsect1>
</refentry>
