<section xml:id="readline.configuration" xmlns="http://docbook.org/ns/docbook">
 &reftitle.runtime;
 &extension.runtime;
 <para>
  <table>
   <title>&ConfigureOptions; da extensão Readline</title>
   <tgroup cols="4">
    <thead>
     <row>
      <entry>&Name;</entry>
      <entry>&Default;</entry>
      <entry>&Changeable;</entry>
      <entry>&Changelog;</entry>
     </row>
    </thead>
    <tbody xml:id="readline.configuration.list">
     <row>
      <entry><link linkend="ini.cli.pager">cli.pager</link></entry>
      <entry>""</entry>
      <entry><constant>INI_ALL</constant></entry>
      <entry></entry>
     </row>
     <row>
      <entry><link linkend="ini.cli.prompt">cli.prompt</link></entry>
      <entry>"\\b \\> "</entry>
      <entry><constant>INI_ALL</constant></entry>
      <entry></entry>
     </row>
    </tbody>
   </tgroup>
  </table>
 </para>

 &ini.descriptions.title;

 <para>
  <variablelist>
   <varlistentry xml:id="ini.cli.pager">
    <term>
     <parameter>cli.pager</parameter>
     <type>string</type>
    </term>
    <listitem>
     <para>
      Ferramenta externa para mostrar a saída da
      <link linkend="features.commandline">linha de comando</link>.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="ini.cli.prompt">
    <term>
     <parameter>cli.prompt</parameter>
     <type>string</type>
    </term>
    <listitem>
     <para>
      Prompt da <link linkend="features.commandline">linha de comando</link>.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </para>
</section>

