<refentry xml:id="mysql-xdevapi-databaseobject.getsession" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>DatabaseObject::getSession</refname>
  <refpurpose>Obtém o nome da sessão</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>abstract</modifier> <modifier>public</modifier> <type>mysql_xdevapi\Session</type><methodname>mysql_xdevapi\DatabaseObject::getSession</methodname>
   <void />
  </methodsynopsis>
  <para>
   Busca a sessão associada ao objeto de banco de dados.
  </para>

  &warn.undocumented.func;

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   O objeto Session.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <example>
   <title>Exemplo de <function>mysql_xdevapi\DatabaseObject::getSession</function></title>
   <programlisting role="php">
<![CDATA[
<?php

$session = $dbObj->getSession();

?>
]]>
   </programlisting>
  </example>
 </refsect1>


</refentry>

