<refentry xml:id="function.tidy-get-output" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>tidy_get_output</refname>
  <refpurpose>Retorna uma string representando o markup analisado pelo tidy</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>tidy_get_output</methodname>
   <methodparam><type>tidy</type><parameter>object</parameter></methodparam>
  </methodsynopsis>
  <para>
   Obtém uma string com o html reparado.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>object</parameter></term>
     <listitem>
      <para>
       O objeto do tidy.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna o markup do tidy analisado.
  </para>
 </refsect1>
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemplo da <function>tidy_get_output</function></title>
    <programlisting role="php">
<![CDATA[
<?php

$html = '<p>paragraph</i>';
$tidy = tidy_parse_string($html);

$tidy->CleanRepair();

echo tidy_get_output($tidy);
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 3.2//EN">
<html>
<head>
<title></title>
</head>
<body>
<p>paragraph</p>
</body>
</html>
]]>
    </screen>
   </example>
  </para>
 </refsect1>
</refentry>

