<refentry xml:id="domnodelist.count" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>DOMNodeList::count</refname>
  <refpurpose>Obtém o número de nós na lista</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="DOMNodeList">
   <modifier>public</modifier> <type>int</type><methodname>DOMNodeList::count</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Obtém o número de nós na lista.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna o número de nós na lista, que é idêntico à propriedade
   <link linkend="domnodelist.props.length">length</link>.
  </para>
 </refsect1>

</refentry>
