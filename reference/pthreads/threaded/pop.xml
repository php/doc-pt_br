<refentry xml:id="threaded.pop" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Threaded::pop</refname>
  <refpurpose>Manipulação</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>bool</type><methodname>Threaded::pop</methodname>
   <void />
  </methodsynopsis>
  <para>
   Retira um item da tabela de propriedades do objeto
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   O último item da tabela de propriedades dos objetos
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Removendo o último item da tabela de propriedades de um objeto Threaded</title>
    <programlisting role="php">
<![CDATA[
<?php
$safe = new Threaded();

while (count($safe) < 10)
    $safe[] = count($safe);

var_dump($safe->pop());
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
int(9)
]]>
    </screen>
   </example>
  </para>
 </refsect1>
</refentry>

