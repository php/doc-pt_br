<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="threaded.count">
 <refnamediv>
  <refname>Threaded::count</refname>
  <refpurpose>Manipulação</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>Threaded::count</methodname>
   <void/>
  </methodsynopsis>
  <simpara>
   Retorna o número de propriedades para este objeto
  </simpara>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>

  </simpara>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <example>
   <title>Contando as propriedades de um objeto</title>
   <programlisting role="php">
<![CDATA[
<?php
$safe = new Threaded();

while (count($safe) < 10) {
    $safe[] = count($safe);
}

var_dump(count($safe));
?>
]]>
   </programlisting>
   &example.outputs;
   <screen>
<![CDATA[
int(10)
]]>
   </screen>
  </example>
 </refsect1>

</refentry>
