<appendix xml:id="mongodb.constants" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 &reftitle.constants;
 &extension.constants;
 <para>
  <variablelist>
   <varlistentry xml:id="constant.mongodb-version">
    <term>
     <constant>MONGODB_VERSION</constant>
     (<type>string</type>)
    </term>
    <listitem>
     <simpara>
      Número da versão do estilo x.y.z da extensão
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="constant.mongodb-stability">
    <term>
     <constant>MONGODB_STABILITY</constant>
     (<type>string</type>)
    </term>
    <listitem>
     <simpara>
      Estabilidade atual (alpha/beta/stable)
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </para>
</appendix>

