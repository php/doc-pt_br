 <refnamediv>
  <refname>MongoDB\Driver\Cursor::valid</refname>
  <refpurpose>Verifica se a posição atual no cursor é válida</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>bool</type><methodname>MongoDB\Driver\Cursor::valid</methodname>
   <void/>
  </methodsynopsis>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &true; se a posição atual do cursor for válida, &false; caso contrário.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
