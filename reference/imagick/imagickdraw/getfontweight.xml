<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagickdraw.getfontweight">
 <refnamediv>
  <refname>ImagickDraw::getFontWeight</refname>
  <refpurpose>Retorna a espessura da fonte</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>ImagickDraw::getFontWeight</methodname>
   <void/>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   Retorna a espessura da fonte usada ao fazer anotações com texto.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna um &integer; em caso de sucesso e 0 se nenhum peso for definido.
  </para>
 </refsect1>

</refentry>

