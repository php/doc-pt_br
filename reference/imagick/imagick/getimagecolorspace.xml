<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagick.getimagecolorspace">
 <refnamediv>
  <refname>Imagick::getImageColorspace</refname>
  <refpurpose>Obtém o espaço de cores da imagem</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>Imagick::getImageColorspace</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Obtém o espaço de cores da imagem.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna um número inteiro que pode ser comparado com as <link linkend="imagick.constants.colorspace">constantes de espaço de cores</link>.
  </para>
 </refsect1>

</refentry>

