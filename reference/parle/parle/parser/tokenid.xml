<refentry xml:id="parle-parser.tokenid" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Parle\Parser::tokenId</refname>
  <refpurpose>Obt√©m o ID do token</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>Parle\Parser::tokenId</methodname>
   <methodparam><type>string</type><parameter>tok</parameter></methodparam>
  </methodsynopsis>
  <para>
   Recupera o ID do token nomeado.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>tok</parameter></term>
    <listitem>
     <para>
      Name of the token as used in <methodname>Parle\Parser::token</methodname>.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna um &integer; representando o ID do token.
  </para>
 </refsect1>


</refentry>

