<refentry xml:id="function.timezone-version-get" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>timezone_version_get</refname>
  <refpurpose>
   Recupera a versão do timezonedb
  </refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>timezone_version_get</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Recupera a versão atual do timezonedb.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna uma <type>string</type>.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Recuperando a versão do timezonedb</title>
    <programlisting role="php">
<![CDATA[
<?php
echo timezone_version_get();
?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
2009.7
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><link linkend="timezones">Lista de fusos horários suportados</link></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

