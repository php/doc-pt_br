<refentry xml:id="weakmap.offsetget" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>WeakMap::offsetGet</refname>
  <refpurpose>Retorna o valor apontado por um certo objeto</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="WeakMap">
   <modifier>public</modifier> <type>mixed</type><methodname>WeakMap::offsetGet</methodname>
   <methodparam><type>object</type><parameter>object</parameter></methodparam>
  </methodsynopsis>
  <para>
   Retorna o valor apontado por um certo objeto.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>object</parameter></term>
    <listitem>
     <para>
      Algum objeto contido como chave em um mapa.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retorna o valor associado ao objeto passado como argumento.
  </para>
 </refsect1>

 <refsect1 role="errors"><!-- {{{ -->
  &reftitle.errors;
  <para>
   Lança uma exceção da classe <classname>Error</classname> em caso de falha.
  </para>
 </refsect1>

</refentry>
